package domain;

public class PagingVO {
	private int pageNo; //현재 화면에 출력되는 페이지네이션 번호
	private int qty; //한 페이지당 보여줄 게시글 수
	
	public PagingVO() { //페이지네이션을 클릭하기 전 기본 리스트 출력 값
		this(1,10);
	}

	public PagingVO(int pageNo, int qty) { //클릭하면 설정되는 값
		this.pageNo = pageNo;
		this.qty = qty;
	}
	
	
	

}
